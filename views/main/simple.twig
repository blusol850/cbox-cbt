<!DOCTYPE html>
<html lang="en">
	<head>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<!-- Latest compiled and minified CSS -->
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

		<title>{{ rc.websiteTitle }}</title>
	</head>
	<body class="container">
		<div class="well">
		{{ rc.content }}
		</div>

		<div class="jumbotron">
		  <h1>{{ rc.complex.hero | upper }}</h1>
		  <p>{{ rc.complex.body }}</p>
		  
		  <p>
		  	<a class="btn btn-primary btn-lg" href="{{ prc.xehLearnMore }}" role="button">Learn more</a>
		  </p>
		</div>

		<h2>Users</h2>
		<table class="table table-hover table-condensed table-striped">
			<tr>
				<th>FirstName</th>
				<th>LastName</th>
			</tr>
			{% for user in rc.users %}
			    <tr>
					<td>{{ user.firstName }}</td>
					<td>{{ user.lastName }}</td>
			    </tr>
			{% endfor %}
		</table>

		<h2>HTTP Request Data</h2>
		<table class="table table-hover table-condensed table-striped">
			<tr>
				<th>Key</th>
				<th>Value</th>
			</tr>
			{% for entry in httpData %}
			    <tr>
					<td>{{ entry.key }}</td>
					<td>{{ entry.value }}</td>
			    </tr>
			{% endfor %}
		</table>

		<h2>CGI</h2>
		<table class="table table-hover table-condensed table-striped">
			<tr>
				<th>Key</th>
				<th>Value</th>
			</tr>
			{% for entry in cgi %}
			    <tr>
					<td>{{ entry.key }}</td>
					<td>{{ entry.value }}</td>
			    </tr>
			{% endfor %}
		</table>

		<h2>SERVER</h2>
		<table class="table table-hover table-condensed table-striped">
			<tr>
				<th>Key</th>
				<th>Value</th>
			</tr>
			{% for entry in SERVER %}
			    <tr>
					<td>{{ entry.key }}</td>
					<td>{{ entry.value }}</td>
			    </tr>
			{% endfor %}
		</table>

		<h2>request</h2>
		<table class="table table-hover table-condensed table-striped">
			<tr>
				<th>Key</th>
				<th>Value</th>
			</tr>
			{% for entry in request %}
			    <tr>
					<td>{{ entry.key }}</td>
					<td>{{ entry.value }}</td>
			    </tr>
			{% endfor %}
		</table>

		<h2>session</h2>
		<table class="table table-hover table-condensed table-striped">
			<tr>
				<th>Key</th>
				<th>Value</th>
			</tr>
			{% for entry in session %}
			    <tr>
					<td>{{ entry.key }}</td>
					<td>{{ entry.value }}</td>
			    </tr>
			{% endfor %}
		</table>

		<div class="footer">
		{{ prc.footer }} 
		</div>
	</body>
</html>